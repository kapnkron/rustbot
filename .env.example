# YubiKey Configuration
YUBIKEY_CLIENT_ID=your_client_id
YUBIKEY_SECRET_KEY=your_secret_key

# Security Settings
REQUIRE_YUBIKEY_FOR_TRADES=true
SESSION_TIMEOUT=1800

# API Keys
DEXTOOLS_API_KEY=your_dextools_api_key
DEXTOOLS_BACKUP_API_KEYS=backup_key1,backup_key2
COINGECKO_API_KEY=your_coingecko_api_key
COINGECKO_BACKUP_API_KEYS=backup_key1,backup_key2

# API URLs
DEXTOOLS_API_URL=https://api.dextools.io/v1
COINGECKO_API_URL=https://api.coingecko.com/api/v3

# Security Settings
ALLOWED_IPS=127.0.0.1,192.168.1.1
REQUEST_TIMEOUT=30000
RATE_LIMIT_REQUESTS=50
RATE_LIMIT_INTERVAL=60000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=project_bolt
DB_USER=postgres
DB_PASSWORD=password

# Application Configuration
PORT=3000
NODE_ENV=development

# Database
DB_PATH=./data/market.db 